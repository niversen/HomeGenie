          "description": {
            "lang": [
              {
                "@xml:lang": "de"
              },
              {
                "@xml:lang": "en",
                "#text": "The calibration value = standard value - measure value.\nE.g. If measure value =800Lux and the standard value = 750Lux, so the\ncalibration value= 750 – 800= -50 (0xFFCE).\nIf the measure value =850Lux and the standard value = 900Lux, so the calibration\nvalue= 900 – 850=50 (0x0032)."
              }
            ]
          }
        },

              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "1°F"
                }
              }
            },
            {
              "@from": "03",
              "@to": "03",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "1.5°F"

            "lang": {
              "@xml:lang": "en",
              "#text": "Battery Type"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Set the type of battery that powers the device. This affects the returned battery state. Default values: 0=3.6V 1=3V\r\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "0 = 3,6 Volt battery"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "1 = 3.0 Volt"
                }
              }
            },

          "@number": "7",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Battery Type"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Set the type of battery that powers the device. This affects the returned battery state.\r\nDefault values:\r\n0=3.6V\r\n1=3V\r\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "0 = 3,6\r\nVolt battery"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "1 = 3.0 Volt"
                }

    "assocGroups": {
      "assocGroup": [
        {
          "@number": "5",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "„Tamper BC” is assigned to the tamper\r\nalarm. Once tampering is detected or cancelled, nodes added to this\r\ngroup will receive alarm frames. This group aims to provide\r\nbackward compatibility with devices not supporting the Z-Wave+\r\nprotocol. Only nodes not supporting the Z-wave+ protocol should be\r\nadded to this group. The „Tamper BC” group can handle up to five\r\ndevices (one or multi-channel). If Fibaro Motion Sensor operates in\r\nprotected mode, outgoing messages are always encrypted."
            }
          }
        },
        {
          "@number": "4",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "„Motion BC” is assigned to the motion\r\nsensor. Once motion is detected or alarm state cancelled, nodes\r\nadded to this group will receive report frames. This group aims to\r\nprovide backward compatibility with controllers not supporting the\r\nZ-Wave+ protocol. Only a main controller not supporting the\r\nZ-wave+ protocol should be added to this group. "
            }
          }
        },
        {
          "@number": "3",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "„Tamper” is assigned to the tamper alarm.\r\nOnce tampering is detected or cancelled, nodes added to this group\r\nwill receive alarm frames. "
            }
          }
        },
        {
          "@number": "2",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "„Motion” is assigned to the motion sensor.\r\nAt the moment of motion detection or alarm state cancellation, nodes\r\nadded to this group will receive control frames. The „Motion” group\r\ncan handle up to five devices (one or multi-channel). If Fibaro Motion\r\nSensor operates in protected mode, outgoing messages are\r\nencrypted, depending on whether the added node also operates in\r\nsecure mode."
            }
          }
        },
        {
          "@number": "1",
          "@maxNodes": "1",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "„Lifeline” is a reporting group, assigned to\r\nthe device status. Main Z-Wave+ network controller should be added\r\nto this group. The „Lifeline” group can handle only one device. If\r\nFibaro Motion Sensor operates in protected mode, outgoing\r\nmessages are always encrypted."
            }
          }
        }
      ]
    },
    "configParams": {
      "configParam": [
        {
          "@number": "1",
          "@type": "range",
          "@size": "2",
          "@default": "000a",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "SENSITIVITY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The lower the value, the more sensitive the PIR sensor."
            }
          },
          "value": {
            "@from": "0008",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the sensor's sensitivity."
              }
            }
          }
        },
        {
          "@number": "2",
          "@type": "range",
          "@size": "1",
          "@default": "0f",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BLIND TIME (INSENSITIVITY)"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Period of time through which the PIR sensor is “blind” (insensitive) to\r\nmotion. After this time period the PIR sensor will be again able to\r\ndetect motion. The longer the insensitivity period, the longer the\r\nbattery life. If the sensor is required to detect motion quickly, the time\r\nperiod may be shortened. The time of insensitivity should be shorter\r\nthat the time period set in parameter 6 (cancellation delay)."
            }
          },
          "value": {
            "@from": "00",
            "@to": "0f",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Available settings: 0 - 15 (0,5 - 8 seconds)\r\nDefine blind time."
              }
            }
          }
        },
        {
          "@number": "3",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "PULSE COUNTER"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Sets the number of moves required for the PIR sensor to report\r\nmotion. The lower the value, the less sensitive the PIR sensor. It’s\r\nnot recommended to modify this parameter settings."
            }
          },
          "value": {
            "@from": "00",
            "@to": "03",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define amount of pulses.\r\nFormula to calculate the number of pulses: pulses = (value + 1)"
              }
            }
          }
        },
        {
          "@number": "4",
          "@type": "range",
          "@size": "1",
          "@default": "02",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "WINDOW TIME"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Period of time during which the number of moves set in parameter 3\r\nmust be detected in order for the PIR sensor to report motion. The\r\nhigher the value, the more sensitive the PIR sensor. It’s not\r\nrecommended to modify this parameter setting.\r\nFormula to calculate the time: time [s] = 4 x (value + 1)"
            }
          },
          "value": {
            "@from": "00",
            "@to": "03",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Set window time."
              }
            }
          }
        },
        {
          "@number": "6",
          "@type": "range",
          "@size": "2",
          "@default": "001e",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "MOTION ALARM CANCELLATION DELAY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Motion alarm will be cancelled in the main controller and the\r\nassociated devices after the period of time set in this parameter. Any\r\nmotion detected during the cancellation delay time countdown will\r\nresult in the countdown being restarted. In case of small values,\r\nbelow 10 seconds, the value of parameter 2 must be modified (PIR\r\nsensor’s “Blind Time”)."
            }
          },
          "value": {
            "@from": "0001",
            "@to": "7fff",
            "@unit": "sec.",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Set parameter for cancellation delay."
              }
            }
          }
        },
        {
          "@number": "8",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "PIR SENSOR OPERATING MODE"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines the part of day in which the PIR sensor\r\nwill be active. This parameter influences only the motion reports and\r\nassociations. Tamper, light intensity and temperature measurements\r\nwill be still active, regardless of this parameter settings."
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "PIR sensor always active"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "PIR sensor active during the day only"
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "PIR sensor active during the night only"
                }
              }
            }
          ]
        },
        {
          "@number": "9",
          "@type": "range",
          "@size": "2",
          "@default": "00c8",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "NIGHT / DAY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter defines the difference between night and day, in\r\nterms of light intensity, used in parameter 8."
            }
          },
          "value": {
            "@from": "0001",
            "@to": "7fff",
            "@unit": "lux",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Set the parameter for light intensity."
              }
            }
          }
        },
        {
          "@number": "12",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BASIC COMMAND CLASS FRAMES CONFIGURATION"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines the command frames sent in 1-st\r\nassociation group, assigned to PIR sensor.\r\nValues of BASIC ON and BASIC OFF command frames may be\r\nmodified by dedicated parameters (14 and 16)."
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "BASIC ON and OFF command frames sent in Basic Command\r\nClass."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Only the BASIC ON command frame sent in Basic Command\r\nClass."
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Only the BASIC OFF command frame sent in Basic Command\r\nClass."
                }
              }
            }
          ]
        },
        {
          "@number": "14",
          "@type": "range",
          "@size": "2",
          "@default": "00ff",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BASIC ON command frame value"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The command frame sent at the moment of motion detection.\r\nFurther motion detections, during the cancellation time, will not result\r\nin sending the association.\r\nThe value other than 0 allows to turn ON an associated device. In\r\ncase of the Dimmer, the value of 255 means turning ON at the last\r\nmemorized state, e.g. the Dimmer turned ON at 30% and turned\r\nOFF using the value of 255, and then turned OFF, will be turned ON\r\nat 30%, i.e. the last memorized state."
            }
          },
          "value": {
            "@from": "0000",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Choose value for BASIC ON command frame."
              }
            }
          }
        },
        {
          "@number": "16",
          "@type": "range",
          "@size": "2",
          "@default": "0000",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BASIC OFF command frame value"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The command frame sent at the moment of motion alarm\r\ncancellation, after cancellation delay time specified in parameter 6.\r\nThe value of 0 allows to turn a device OFF while the value of 255\r\nallows to turn ON a device. In case of the Dimmer, the value of 255\r\nmeans turning ON at the last memorized state, e.g. the Dimmer\r\nturned ON at 30% and turned OFF using the value of 255, and then\r\nturned OFF, will be turned ON at 30%, i.e. the last memorized state."
            }
          },
          "value": {
            "@from": "0000",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Choose value for BASIC OFF command frame."
              }
            }
          }
        },
        {
          "@number": "20",
          "@type": "range",
          "@size": "1",
          "@default": "0f",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER SENSITIVITY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines the changes in forces acting on the Fibaro\r\nMotion Sensor resulting in tamper alarm being reported - g-force\r\nacceleration.\r\nAvailable settings: 0 - 121 (0,08 - 2g; multiply by 0,016g;\r\n0 = tamper inactive)"
            }
          },
          "value": {
            "@from": "00",
            "@to": "79",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define tamper sensitivity."
              }
            }
          }
        },
        {
          "@number": "22",
          "@type": "range",
          "@size": "2",
          "@default": "001e",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER ALARM CANCELLATION DELAY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Time period after which a tamper alarm will be cancelled. Another\r\ntampering detected during the countdown to cancellation will not\r\nextend the delay."
            }
          },
          "value": {
            "@from": "0001",
            "@to": "7fff",
            "@unit": "sec.",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Choose the time period."
              }
            }
          }
        },
        {
          "@number": "24",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER OPERATING MODES"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines the behaviour of tamper alarm and how it\r\nreports. It is an advanced feature serving much more functions than\r\njust detection of tampering."
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "only tamper"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper and earthquake detector. Function may be used as a\r\nsimple seismograph. Two reports including the strength of shock are\r\nalways sent. First, right after the detection and second (value=0)\r\nafter tamper alarm cancellation delay time."
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper and orientation in space. Three reports are sent right\r\nafter tamper alarm cancellation delay time."
                }
              }
            }
          ]
        },
        {
          "@number": "25",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER CANCELLATION REPORTS"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines if a tamper cancellation report is sent."
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Do not send tamper cancellation report."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Send tamper cancellation report."
                }
              }
            }
          ]
        },
        {
          "@number": "28",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER ALARM BROADCAST MODE - 3rd Group"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines whether the tamper alarm frame will or\r\nwill not be sent in broadcast mode. Alarm frames sent in broadcast\r\nmode can be received by all of the devices within communication\r\nrange (if they accept such frames). When Fibaro Motion Sensor is\r\noperating in protected mode (secure mode), the value should remain\r\ndefault. Device operating in protected mode doesn’t send frames in\r\nbroadcast mode."
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper alarm reported to association groups."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper alarm reported in broadcast mode to 3rd association\r\ngroup."
                }
              }
            }
          ]
        },
        {
          "@number": "29",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER ALARM BROADCAST MODE - 5th Group"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines whether the tamper alarm frame will or\r\nwill not be sent in broadcast mode. Alarm frames sent in broadcast\r\nmode may be received by all of the devices within communication\r\nrange (if they accept such frames). When Fibaro Motion Sensor is\r\nrunning in protected mode (secure mode), the value should remain\r\ndefault. Device operating in protected mode do not send frames in\r\nbroadcast mode."
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper alarm reported to association groups."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper alarm reported in broadcast mode to 5th association\r\ngroup."
                }
              }
            }
          ]
        },
        {
          "@number": "40",
          "@type": "range",
          "@size": "2",
          "@default": "00c8",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "ILLUMINATION REPORT THRESHOLD"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines the change in light intensity level\r\nresulting in illumination report being sent to the main controller."
            }
          },
          "value": {
            "@from": "0000",
            "@to": "7fff",
            "@unit": "lux",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the light intensity level.\r\n0=no reports"
              }
            }
          }
        },
        {
          "@number": "42",
          "@type": "range",
          "@size": "2",
          "@default": "0000",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "ILLUMINATION REPORTS INTERVAL"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Time interval between consecutive illumination reports. The reports\r\nare sent even if there are no changes in the light intensity."
            }
          },
          "value": {
            "@from": "0000",
            "@to": "7fff",
            "@unit": "sec",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the time interval.\r\n0=no reports"
              }
            }
          }
        },
        {
          "@number": "60",
          "@type": "range",
          "@size": "2",
          "@default": "000a",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TEMPERATURE REPORT THRESHOLD"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines the change in level of temperature\r\nresulting in temperature report being sent to the main controller."
            }
          },
          "value": {
            "@from": "0000",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define change in level of temperature.\r\nAvailable settings: 0 - 255 (0,1-25,5°C,; 0 = reports are not sent)"
              }
            }
          }
        },
        {
          "@number": "62",
          "@type": "range",
          "@size": "2",
          "@default": "0384",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "INTERVAL OF TEMPERATURE MEASURING"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The parameter determines how often the temperature will be\r\nmeasured. The shorter the time, the more frequently the temperature\r\nwill be measured, but the battery life will shorten."
            }
          },
          "value": {
            "@from": "0000",
            "@to": "7fff",
            "@unit": "sec",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the interval of consecutive measurements.\r\n0 = temperature\r\nwill not be measured"
              }
            }
          }
        },

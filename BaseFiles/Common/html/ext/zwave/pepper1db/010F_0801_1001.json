    "deviceDescription": {
      "description": {
        "lang": {
          "@xml:lang": "en",
          "#text": "Motion Sensor"
        }
      },
      "wakeupNote": {
        "lang": {
          "@xml:lang": "en"
        }
      },
      "inclusionNote": {
        "lang": {
          "@xml:lang": "en",
          "#text": "The Fibaro Motion Sensor can be included into the Z-Wave network\r\nby using the B-button. Quickly, triple click the B-button - LED diode will glow blue to\r\nconfirm setting into learning mode."
        }
      },

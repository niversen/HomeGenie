    "assocGroups": {
      "assocGroup": [
        {
          "@number": "5",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\n„Tamper BC” is assigned to the tamper alarm. Once tampering is detected or cancelled, nodes added to this group will receive alarm frames. This group aims to provide backward compatibility with devices not supporting the Z-Wave+ protocol. Only nodes not supporting the Z-wave+ protocol should be added to this group. The „Tamper BC” group can handle up to five devices (one or multi-channel). If Fibaro Motion Sensor operates in protected mode, outgoing messages are always encrypted.\n"
            }
          }
        },
        {
          "@number": "4",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\n„Motion BC” is assigned to the motion sensor. Once motion is detected or alarm state cancelled, nodes added to this group will receive report frames. This group aims to provide backward compatibility with controllers not supporting the Z-Wave+ protocol. Only a main controller not supporting the Z-wave+ protocol should be added to this group.\n"
            }
          }
        },
        {
          "@number": "3",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\n„Tamper” is assigned to the tamper alarm. Once tampering is detected or cancelled, nodes added to this group will receive alarm frames.\n"
            }
          }
        },
        {
          "@number": "2",
          "@maxNodes": "5",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\n„Motion” is assigned to the motion sensor. At the moment of motion detection or alarm state cancellation, nodes added to this group will receive control frames. The „Motion” group can handle up to five devices (one or multi-channel). If Fibaro Motion Sensor operates in protected mode, outgoing messages are encrypted, depending on whether the added node also operates in secure mode.\n"
            }
          }
        },
        {
          "@number": "1",
          "@maxNodes": "1",
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\n„Lifeline” is a reporting group, assigned to the device status. Main Z-Wave+ network controller should be added to this group. The „Lifeline” group can handle only one device. If Fibaro Motion Sensor operates in protected mode, outgoing messages are always encrypted.\n"
            }
          }
        }
      ]
    },
    "configParams": {
      "configParam": [
        {
          "@number": "1",
          "@type": "range",
          "@size": "2",
          "@default": "000a",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "SENSITIVITY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe lower the value, the more sensitive the PIR sensor.\n"
            }
          },
          "value": {
            "@from": "0008",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the sensor's sensitivity."
              }
            }
          }
        },
        {
          "@number": "2",
          "@type": "range",
          "@size": "1",
          "@default": "0f",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BLIND TIME (INSENSITIVITY)"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nPeriod of time through which the PIR sensor is “blind” (insensitive) to motion. After this time period the PIR sensor will be again able to detect motion. The longer the insensitivity period, the longer the battery life. If the sensor is required to detect motion quickly, the time period may be shortened. The time of insensitivity should be shorter that the time period set in parameter 6 (cancellation delay).\n"
            }
          },
          "value": {
            "@from": "00",
            "@to": "0f",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "\nAvailable settings: 0 - 15 (0,5 - 8 seconds) Define blind time.\n"
              }
            }
          }
        },
        {
          "@number": "3",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "PULSE COUNTER"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nSets the number of moves required for the PIR sensor to report motion. The lower the value, the less sensitive the PIR sensor. It’s not recommended to modify this parameter settings.\n"
            }
          },
          "value": {
            "@from": "00",
            "@to": "03",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "\nDefine amount of pulses. Formula to calculate the number of pulses: pulses = (value + 1)\n"
              }
            }
          }
        },
        {
          "@number": "4",
          "@type": "range",
          "@size": "1",
          "@default": "02",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "WINDOW TIME"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nPeriod of time during which the number of moves set in parameter 3 must be detected in order for the PIR sensor to report motion. The higher the value, the more sensitive the PIR sensor. It’s not recommended to modify this parameter setting. Formula to calculate the time: time [s] = 4 x (value + 1)\n"
            }
          },
          "value": {
            "@from": "00",
            "@to": "03",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Set window time."
              }
            }
          }
        },
        {
          "@number": "6",
          "@type": "range",
          "@size": "2",
          "@default": "001e",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "MOTION ALARM CANCELLATION DELAY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nMotion alarm will be cancelled in the main controller and the associated devices after the period of time set in this parameter. Any motion detected during the cancellation delay time countdown will result in the countdown being restarted. In case of small values, below 10 seconds, the value of parameter 2 must be modified (PIR sensor’s “Blind Time”).\n"
            }
          },
          "value": {
            "@from": "0001",
            "@to": "7fff",
            "@unit": "sec.",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Set parameter for cancellation delay."
              }
            }
          }
        },
        {
          "@number": "8",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "PIR SENSOR OPERATING MODE"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines the part of day in which the PIR sensor will be active. This parameter influences only the motion reports and associations. Tamper, light intensity and temperature measurements will be still active, regardless of this parameter settings.\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "PIR sensor always active"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "PIR sensor active during the day only"
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "PIR sensor active during the night only"
                }
              }
            }
          ]
        },
        {
          "@number": "9",
          "@type": "range",
          "@size": "2",
          "@default": "00c8",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "NIGHT / DAY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter defines the difference between night and day, in terms of light intensity, used in parameter 8.\n"
            }
          },
          "value": {
            "@from": "0001",
            "@to": "7fff",
            "@unit": "lux",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Set the parameter for light intensity."
              }
            }
          }
        },
        {
          "@number": "12",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BASIC COMMAND CLASS FRAMES CONFIGURATION"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines the command frames sent in 1-st association group, assigned to PIR sensor. Values of BASIC ON and BASIC OFF command frames may be modified by dedicated parameters (14 and 16).\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nBASIC ON and OFF command frames sent in Basic Command Class.\n"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nOnly the BASIC ON command frame sent in Basic Command Class.\n"
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nOnly the BASIC OFF command frame sent in Basic Command Class.\n"
                }
              }
            }
          ]
        },
        {
          "@number": "14",
          "@type": "range",
          "@size": "2",
          "@default": "00ff",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BASIC ON command frame value"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe command frame sent at the moment of motion detection. Further motion detections, during the cancellation time, will not result in sending the association. The value other than 0 allows to turn ON an associated device. In case of the Dimmer, the value of 255 means turning ON at the last memorized state, e.g. the Dimmer turned ON at 30% and turned OFF using the value of 255, and then turned OFF, will be turned ON at 30%, i.e. the last memorized state.\n"
            }
          },
          "value": {
            "@from": "0000",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Choose value for BASIC ON command frame."
              }
            }
          }
        },
        {
          "@number": "16",
          "@type": "range",
          "@size": "2",
          "@default": "0000",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "BASIC OFF command frame value"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe command frame sent at the moment of motion alarm cancellation, after cancellation delay time specified in parameter 6. The value of 0 allows to turn a device OFF while the value of 255 allows to turn ON a device. In case of the Dimmer, the value of 255 means turning ON at the last memorized state, e.g. the Dimmer turned ON at 30% and turned OFF using the value of 255, and then turned OFF, will be turned ON at 30%, i.e. the last memorized state.\n"
            }
          },
          "value": {
            "@from": "0000",
            "@to": "00ff",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Choose value for BASIC OFF command frame."
              }
            }
          }
        },
        {
          "@number": "20",
          "@type": "range",
          "@size": "1",
          "@default": "0f",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER SENSITIVITY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines the changes in forces acting on the Fibaro Motion Sensor resulting in tamper alarm being reported - g-force acceleration. Available settings: 0 - 121 (0,08 - 2g; multiply by 0,016g; 0 = tamper inactive)\n"
            }
          },
          "value": {
            "@from": "00",
            "@to": "79",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define tamper sensitivity."
              }
            }
          }
        },
        {
          "@number": "22",
          "@type": "range",
          "@size": "2",
          "@default": "001e",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER ALARM CANCELLATION DELAY"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nTime period after which a tamper alarm will be cancelled. Another tampering detected during the countdown to cancellation will not extend the delay.\n"
            }
          },
          "value": {
            "@from": "0001",
            "@to": "7fff",
            "@unit": "sec.",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Choose the time period."
              }
            }
          }
        },
        {
          "@number": "24",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER OPERATING MODES"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines the behaviour of tamper alarm and how it reports. It is an advanced feature serving much more functions than just detection of tampering.\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "only tamper"
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nTamper and earthquake detector. Function may be used as a simple seismograph. Two reports including the strength of shock are always sent. First, right after the detection and second (value=0) after tamper alarm cancellation delay time.\n"
                }
              }
            },
            {
              "@from": "02",
              "@to": "02",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nTamper and orientation in space. Three reports are sent right after tamper alarm cancellation delay time.\n"
                }
              }
            }
          ]
        },
        {
          "@number": "25",
          "@type": "constant",
          "@size": "1",
          "@default": "01",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER CANCELLATION REPORTS"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines if a tamper cancellation report is sent.\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Do not send tamper cancellation report."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Send tamper cancellation report."
                }
              }
            }
          ]
        },
        {
          "@number": "28",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER ALARM BROADCAST MODE - 3rd Group"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines whether the tamper alarm frame will or will not be sent in broadcast mode. Alarm frames sent in broadcast mode can be received by all of the devices within communication range (if they accept such frames). When Fibaro Motion Sensor is operating in protected mode (secure mode), the value should remain default. Device operating in protected mode doesn’t send frames in broadcast mode.\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper alarm reported to association groups."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nTamper alarm reported in broadcast mode to 3rd association group.\n"
                }
              }
            }
          ]
        },
        {
          "@number": "29",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TAMPER ALARM BROADCAST MODE - 5th Group"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines whether the tamper alarm frame will or will not be sent in broadcast mode. Alarm frames sent in broadcast mode may be received by all of the devices within communication range (if they accept such frames). When Fibaro Motion Sensor is running in protected mode (secure mode), the value should remain default. Device operating in protected mode do not send frames in broadcast mode.\n"
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Tamper alarm reported to association groups."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "\nTamper alarm reported in broadcast mode to 5th association group.\n"
                }
              }
            }
          ]
        },
        {
          "@number": "40",
          "@type": "range",
          "@size": "2",
          "@default": "00c8",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "ILLUMINATION REPORT THRESHOLD"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nThe parameter determines the change in light intensity level resulting in illumination report being sent to the main controller.\n"
            }
          },
          "value": {
            "@from": "0000",
            "@to": "7fff",
            "@unit": "lux",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the light intensity level. 0=no reports"
              }
            }
          }
        },
        {
          "@number": "42",
          "@type": "range",
          "@size": "2",
          "@default": "0000",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "ILLUMINATION REPORTS INTERVAL"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "\nTime interval between consecutive illumination reports. The reports are sent even if there are no changes in the light intensity.\n"
            }
          },
          "value": {
            "@from": "0000",
            "@to": "7fff",
            "@unit": "sec",
            "description": {
              "lang": {
                "@xml:lang": "en",
                "#text": "Define the time interval. 0=no reports"
              }
            }
          }
        },
        {
          "@number": "60",
          "@type": "range",
          "@size": "2",
          "@default": "000a",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "TEMPERATURE REPORT THRESHOLD"
            }
          },
          "description": {
            "lang": {

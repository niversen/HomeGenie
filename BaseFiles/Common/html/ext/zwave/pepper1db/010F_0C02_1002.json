            "lang": {
              "@xml:lang": "en",
              "#text": "Associations in Z-Wave network security mode"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "This parameter defines how commands are sent in specified association groups:\r\nas secure or non-secure. Parameter is active only in Z-Wave network security\r\nmode. It doesnâ€™t apply to 1st Lifeline group."
            }

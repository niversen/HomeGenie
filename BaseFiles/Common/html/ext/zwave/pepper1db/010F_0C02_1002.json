          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "The temperature report will only be sent if there is a difference in temperature value\r\nfrom the previous value reported, defined in this parameter (hysteresis).\r\nTemperature reports can be also sent as a result of polling.\r\nAvailable settings: 1 â€“ 100 (in 0,1Â°C steps)"
            }
          },
          "value": {
            "@from": "01",
            "@to": "64",
            "description": {
              "lang": {
                "@xml:lang": "en",

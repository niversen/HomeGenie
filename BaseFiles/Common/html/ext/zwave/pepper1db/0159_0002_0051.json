          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Temperatursensor-Offset-Einstellungen"
              },
              {
                "@xml:lang": "en",
                "#text": "Temperature sensor offset settings"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Der eingestellte Wert wird zum aktuell gemessenen Sensorwert dazu gerechnet oder von diesem abgezogen.\r\n"
              },
              {
                "@xml:lang": "en",
                "#text": "Set value is added or subtracted to actual measured value by sensor.\r\n"
              }
            ]
          },
          "value": [
            {
              "@from": "0000",
              "@to": "0000",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "Offset beträgt 0.0°C"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "offset is 0.0°C"
                  }
                ]
              }
            },
            {
              "@from": "0001",
              "@to": "0064",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "\nWert zwischen 0.1°C und 10.0°C wird zur aktuell gemessenen Temperatur hinzuaddiert\n"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "\nValue from 0.1 °C to 10.0 °C is added to actual measured temperature.\n"
                  }
                ]
              }
            },
            {
              "@from": "03e9",
              "@to": "044c",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de",
                    "#text": "\nWert zwischen -0.1°C und -10.0°C wird von der aktuell gemessenen Temperatur subtrahiert.\n"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "\nValue from -0.1 °C to -10.0 °C is subtracted to actual measured temperature.\n"
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "120",
          "@type": "rangemapped",
          "@size": "1",
          "@default": "05",
          "name": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Digitaler Temperatursensorbericht"
              },
              {
                "@xml:lang": "en",
                "#text": "Digital temperature sensor reporting"
              }
            ]
          },
          "description": {
            "lang": [
              {
                "@xml:lang": "de",
                "#text": "Wenn der digitale Temperatursensor angeschlossen ist, meldet das Modul die gemessene Temperatur bei durch den Parameter definiertem Temperaturwechsel\r\n"
              },
              {
                "@xml:lang": "en",
                "#text": "If digital temperature sensor is connected, module reports measured temperature on temperature change defined by this parameter.\r\n"
              }
            ]
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Reporting disabled"
                  }
                ]
              }
            },
            {
              "@from": "01",
              "@to": "7f",
              "@unit": "x 0,1°C",
              "description": {
                "lang": [
                  {
                    "@xml:lang": "de"
                  },
                  {
                    "@xml:lang": "en",
                    "#text": "Define the temperature change that is reported."
                  }
                ]
              }
            }
          ]
        },
        {
          "@number": "13",
          "@type": "rangemapped",
          "@size": "2",
          "@default": "0000",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Automatic turning off output Q2 after set time "
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "When relay Q2 is ON it goes automatically OFF after time defined by this parameter. Timer is reset to zero each time the module receive ON command regardless from where it comes (push button, associated module, gateway,..).  "
            }
          },
          "value": [
            {
              "@from": "0000",
              "@to": "0000",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Auto OFF disabled "
                }
              }
            },
            {
              "@from": "0001",
              "@to": "7f17",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "1second (0,01s) – 32535 seconds (325,35s) Auto OFF enabled with define time, step is 1s or 10ms according to parameter nr.15"
                }
              }
            }
          ]
        },
        {
          "@number": "14",
          "@type": "rangemapped",
          "@size": "2",
          "@default": "0000",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Automatic turning on output Q2 after set time "
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "When relay Q2 is OFF it goes automatically ON after time defined by this parameter. Timer is reset to zero each time the module receive OFF command regardless from where it comes (push button, associated module, gateway,..).  "
            }
          },
          "value": [
            {
              "@from": "0000",
              "@to": "0000",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Auto ON disabled "
                }
              }
            },
            {
              "@from": "0001",
              "@to": "7f17",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "1second (0,01s) – 32536 seconds (325,35s) Auto ON enabled with define time, step is 1s or 10ms according to parameter nr.15"
                }
              }
            }
          ]
        },
        {
          "@number": "40",
          "@type": "rangemapped",
          "@size": "1",
          "@default": "0a",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Power reporting in Watts on power change for Q1"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Set value means percentage, set value from 0 – 100 = 0% - 100%. "
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Reporting Disabled "
                }
              }
            },
            {
              "@from": "01",
              "@to": "64",
              "@unit": "%",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Power report is send (push) only when actual power in Watts in real time changes for more than set percentage comparing to previous actual power in Watts, step is 1%."
                }
              }
            }
          ]
        },
        {
          "@number": "41",
          "@type": "rangemapped",
          "@size": "1",
          "@default": "0a",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Power reporting in Watts on power change for Q2"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Set value means percentage, set value from 0 – 100 = 0% - 100%. "
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en"
                }
              }
            },
            {
              "@from": "01",
              "@to": "64",
              "@unit": "%",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Reporting enabled. Power report is send (push) only when actual power in Watts in real time changes for more than set percentage comparing to previous actual power in Watts, step is 1%."
                }
              }
            }
          ]
        },
        {
          "@number": "42",
          "@type": "rangemapped",
          "@size": "2",
          "@default": "012c",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Power reporting in Watts by time interval for Q1"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Set value means time interval (0 – 32535) in seconds, when power report is send. "
            }
          },
          "value": [
            {
              "@from": "0000",
              "@to": "0000",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Reporting Disabled"
                }
              }
            },
            {
              "@from": "0001",
              "@to": "7f17",
              "@unit": "sec",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Reporting enabled, Power report is send with time interval set by entered value."
                }
              }
            }
          ]
        },
        {
          "@number": "43",
          "@type": "rangemapped",
          "@size": "2",
          "@default": "012c",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Power reporting in Watts by time interval for Q2"
            }
          },
          "description": {
            "lang": {

                "lang": {
                  "@xml:lang": "en",
                  "#text": "Reporting Disabled"
                }
              }
            },
            {
              "@from": "0001",
              "@to": "7f17",
              "@unit": "sec",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "Reporting enabled, Power report is send with time interval set by entered value. "
                }
              }
            }
          ]
        },
        {
          "@number": "64",
          "@type": "constant",
          "@size": "1",
          "@default": "00",
          "name": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Output Q2 Switch selection"
            }
          },
          "description": {
            "lang": {
              "@xml:lang": "en",
              "#text": "Set value means the type of the device that is connected to the PWM output. The device type can be normally open (NO) or normally close (NC).  "
            }
          },
          "value": [
            {
              "@from": "00",
              "@to": "00",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "When system is turned off the output is 0V (NC)."
                }
              }
            },
            {
              "@from": "01",
              "@to": "01",
              "description": {
                "lang": {
                  "@xml:lang": "en",
                  "#text": "When system is turned off the output is 230V (NO)."
                }
              }
            }
          ]
        }
